{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^5.0.12",
    "@antv/g2": "^4.0.15",
    "@antv/g6": "^3.8.1",
    "@antv/l7": "^2.2.37",
    "@antv/l7-maps": "^2.2.37",
    "@antv/l7-react": "^2.2.39",
    "@types/md5": "^2.2.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.22",
    "lint-staged": "^10.0.7",
    "md5": "^2.3.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.22",
    "yorkie": "^2.0.0"
  }
}
